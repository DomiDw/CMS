{
  "name": "aisw-cms",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@google-cloud/debug-agent": "^5.0.0",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@soccerwatch/discovery": "^1.1.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@tippyjs/react": "^4.0.2",
    "@types/jest": "^26.0.0",
    "@types/node": "12.12.37",
    "axios": "^0.19.2",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^8.2.0",
    "flexboxgrid": "6.3.1",
    "inspector": "^0.5.0",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-player": "2.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "4.0.5",
    "standardx": "^5.0.0",
    "tippy.js": "^6.2.3",
    "typescript": "3.7.5",
    "video-react": "0.14.1",
    "vscode-react-typescript": "^1.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "buildProd": "dotenv -e .env.production react-scripts build",
    "buildDev": "dotenv -e .env.development react-scripts build",
    "test": "react-scripts test && standard && node my-tests.js",
    "eject": "react-scripts eject",
    "uploadDev": "gcloud beta functions deploy aisw-cms --region=europe-west1 --project=sw-sc-de-dev --env-vars-file=dev.yaml --runtime nodejs10 --trigger-http",
    "uploadProd": "gcloud beta functions deploy aisw-cms --region=europe-west1 --project=sw-sc-de-prod --env-vars-file=prod.yaml --runtime nodejs10 --trigger-http",
    "deployDev": "npm run buildDev && npm run uploadDev",
    "deployProd": "npm run buildProd && npm run uploadProd"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "babel-eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "dotenv-cli": "^1.4.0"
  }
}
